__version__ = "2020.10.09"

import rhinoscriptsyntax as rs
import Rhino.Geometry as rg
import ghpythonlib.components as gh

from compas_rhino import unload_modules
unload_modules('model')

from model import Brick



def curve_to_planes():
    brick_planes = []
    for curve in curves:
        params = curve.DivideByLength(Brick.REFERENCE_LENGTH, False)
        for param in params:
            brick_pos = rg.Curve.PointAt(curve, param)
            brick_tan = rg.Curve.TangentAt(curve, param) # follow like tangenAt
            vec1 = rg.Vector3d.CrossProduct(brick_tan, rg.Vector3d.ZAxis)
            vec2 = rg.Vector3d.CrossProduct(vec1, rg.Vector3d.ZAxis)
            
            plane = rg.Plane(brick_pos,vec1, vec2)
            brick_planes.append(plane)
    return brick_planes

planes = curve_to_planes()
