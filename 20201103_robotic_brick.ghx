    x = (Brick.REFERENCE_LENGTH/2 + Brick.REFERENCE_WIDTH/2)
    y = (Brick.REFERENCE_WIDTH+0.5)
    z = Brick.REFERENCE_HEIGHT 
    for i in range(row):
