                    <item name="CodeInput" type_name="gh_string" type_code="10">__author__ = "WeiTingChen"
__version__ = "2020.10.09"

from compas_rhino import unload_modules
unload_modules('model')
import Rhino.Geometry as rg
from model import Brick
import math as m


def wall(row, column):
    planes = []
    planes_base = []
    for i in range(column):
        for j in range(row):
            if i % 2 ==1:
                x_value = (Brick.REFERENCE_LENGTH + 1) * j + (Brick.REFERENCE_LENGTH/2)
            else:
                x_value = (Brick.REFERENCE_LENGTH + 1) * j
            z_value = (Brick.REFERENCE_HEIGHT) * i
            plane_origin = rg.Point3d(x_value, 0, z_value)
            plane = rg.Plane(plane_origin, rg.Vector3d.XAxis, rg.Vector3d.YAxis)
            planes_base.append(plane)
            myBrick = Brick(plane)
            planes.append(myBrick.picking_plane())
    return planes, planes_base

def rotate(plane,attractorCrv,scale):
    new_plane = []
    # get the closet points from points
    success, t = attractorCrv.ClosestPoint(plane.Origin)
    crv_pt = attractorCrv.PointAt(t)
    distance = plane.Origin.DistanceTo(crv_pt)
    #distance = attractor.DistanceTo(plane.Origin)
    rotation = m.radians(distance * scale)
    new_plane.append(plane.Rotate(rotation, rg.Vector3d.ZAxis))
    return new_plane

def get_model():
    planes, planes_base = wall(row, column)
    
    # get fabrcation base
    f_planes = []
    for plane in planes:
        rotate(plane,attractorCrv,scale)
        f_planes.append(plane)
        
    meshes =[]
    # create mesh by planes_bas
    for plane_base in planes_base:
        rotate(plane_base,attractorCrv,scale)
        wallBrick = Brick(plane_base)
        meshes.append(wallBrick.mesh())
    return meshes, f_planes


