import rhinoscriptsyntax as rs
import Rhino.Geometry as rg
import math

# spiral
points =[]
points02 = []
for i in range(count):
    randian = math.exp(i*0.01)
    spiral = math.pi * 2 * i *0.01
    points.append(rg.Point3d(
                  randian*math.cos(spiral), randian*math.sin(spiral), i*height))

for i in range(count):
    randian = math.exp(i*0.01)
    spiral = math.pi * 2 * i *0.01
    points02.append(rg.Point3d(
                  randian*math.cos(spiral), randian*math.sin(spiral), 0))


curve01 = [rg.Curve.CreateControlPointCurve(points[20:count], 3),
          rg.Curve.CreateControlPointCurve(points02[20:count], 3)]


srf = rg.Brep.CreateFromLoft(curve01, rg.Point3d.Unset, rg.Point3d.Unset, rg.LoftType.Normal, False)</item>
