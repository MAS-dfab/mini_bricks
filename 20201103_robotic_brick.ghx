__version__ = "2020.10.09"

from compas_rhino import unload_modules
unload_modules('model')
import Rhino.Geometry as rg
from model import Brick


planes = []
meshes =[]


for i in range(10):
    for j in range(5):
        if i % 2 ==1:
            x_value = (Brick.REFERENCE_LENGTH + x_gap) * j + (Brick.REFERENCE_LENGTH/2)
            z_value = (Brick.REFERENCE_HEIGHT + z_gap) * i
        else:
            x_value = (Brick.REFERENCE_LENGTH + x_gap) * j
            z_value = (Brick.REFERENCE_HEIGHT + z_gap) * i
        plane_origin = rg.Point3d(x_value, 0, z_value)
        plane = rg.Plane(plane_origin, rg.Vector3d.XAxis, rg.Vector3d.YAxis)
        
        myBrick = Brick(plane)
        planes.append(myBrick.picking_plane())
        meshes.append(myBrick.mesh())


